  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.0",
    "@vitest/coverage-c8": "^0.28.5",
    "git-commit-msg-linter": "^4.7.3",
    "husky": "^8.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  },
